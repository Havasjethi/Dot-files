{
  "workbench.iconTheme": "material-icon-theme",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.tabSize": 2,
  "editor.renderWhitespace": "none",
  "breadcrumbs.enabled": true,
  "editor.minimap.enabled": true,
  "editor.renderControlCharacters": false,
  "[markdown]": {
    "editor.wordWrap": "off",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "workbench.sideBar.location": "right",
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.autoIndent": "advanced"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    // "source.": false  
  },
  "todohighlight.defaultStyle": {"color": "red", "backgroundColor": "purple", "overviewRulerColor": "black"},
  "editor.fontSize": 16,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "prettier.ignorePath": "",
  "json.maxItemsComputed": 500,
  "terminal.integrated.tabs.enabled": true,
  "gitlens.mode.active": "zen",
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "terminal.explorerKind": "external",
  "terminal.external.linuxExec": "fish",
  "vva.trustedDomain": true,
  "vscode-neovim.neovimExecutablePaths.win32/linux/darwin": "/snap/bin/nvim",
  "vscode-neovim.neovimExecutablePaths.linux": "/snap/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.linux": "/home/havas/.config/nvim/init.vim",
  "editor.lineNumbers": "relative",
  "editor.suggest.showStatusBar": true,
  "todohighlight.keywordsPattern": "[Tt][Oo][Dd][Oo] :: (.)*",
  "git.confirmSync": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[jsonc]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[vue]": {
    "editor.defaultFormatter": "johnsoncodehk.volar"
  },
  "latex-workshop.view.pdf.viewer": "browser",
  "rust-analyzer.completion.privateEditable.enable": true,
  "[bibtex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop"
  },
  "editor.wordWrap": "on",
  "rust-analyzer.inlayHints.closureReturnTypeHints.enable": true
}
