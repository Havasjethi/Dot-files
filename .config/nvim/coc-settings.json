{
  "suggest.noselect": false,

  "prettier.enable": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "json",
    "rust",
    "css",
    "html",
    "markdown",
    "vue"
  ],
  "languageserver": {
    "angular": {
      "command": "",
      "rootPatterns": ["angular.json", "frontend/angular.json"],
      "filetypes": ["*.ts", "*.html", "*.scss"]
    }
  },
  "rust-analyzer.inlayHints.chainingHintsSeparator": " ",
  "typescript.suggest.completeFunctionCalls": true,
  "sql.database": "postgresql",
  "cSpell.userWords": [
    "HAVAS",
    "Timestep",
    "Vsync",
    "copysign",
    "shellexpand",
    "typeorm",
    "workdir"
  ]
}
